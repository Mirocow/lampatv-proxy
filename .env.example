# Logging
LOG_LEVEL=INFO

# Timeouts
TIMEOUT_CONNECT=10.0
TIMEOUT_READ=30.0
TIMEOUT_WRITE=10.0
TIMEOUT_POOL=10.0

# Proxy Timeouts
TIMEOUT_CONNECT_PROXY=30.0
TIMEOUT_READ_PROXY=300.0
TIMEOUT_WRITE_PROXY=30.0
TIMEOUT_POOL_PROXY=30.0

# Streaming
STREAM_CHUNK_SIZE=65536
STREAM_TIMEOUT=300.0
MAX_RANGE_SIZE=104857600

# Performance
MAX_CONNECTIONS=100
MAX_KEEPALIVE_CONNECTIONS=20
KEEPALIVE_EXPIRY=5.0

# User Agent
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Limits
MAX_REDIRECTS=5
MAX_REQUEST_SIZE=10485760

# Proxy Settings
USE_PROXY=false
PROXY_LIST=proxy1.com:8080,proxy2.com:8080
PROXY_TEST_URL=http://httpbin.org/ip
PROXY_TEST_TIMEOUT=10
MAX_PROXY_RETRIES=3
SESSION_TTL=1800