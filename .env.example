# Включить использование прокси (true/false)
USE_PROXY=false

# Список прокси серверов (через запятую)
PROXY_LIST=http://proxy1.example.com:8080,http://proxy2.example.com:8080,https://proxy3.example.com:443,http://user:pass@proxy4.example.com:8080,socks5://proxy.example.com:1080

# URL для проверки прокси
PROXY_TEST_URL=https://httpbin.org/ip

# Таймаут проверки прокси (секунды)
PROXY_TEST_TIMEOUT=10

# Максимальное количество повторных попыток для прокси
MAX_PROXY_RETRIES=3

# Уровень логирования (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO